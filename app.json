{
  "expo": {
    "name": "react-marketplace",
    "slug": "react-marketplace",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/trolly.png",
      "resizeMode": "contain",
      "backgroundColor": "#8FBC8F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "A marketplace in which pre-owned and no longer wanted items can be sold.",
    "githubUrl": "https://github.com/Fancystacks/react-marketplace"
  }
}
