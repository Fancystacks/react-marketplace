{
  "expo": {
    "name": "react-marketplace",
    "slug": "react-marketplace",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A marketplace in which pre-owned and no longer wanted items can be sold.",
    "githubUrl": "https://github.com/Fancystacks/react-marketplace"
  }
}
